@using TravelTripProject.Models.Sınıflar
@model List<Blog>

<div class="box1-left">
    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.ImageUrl" style="width:140px; height:80px" alt="" />
        </div>
        <div class="bot-desc1">
            <p>@item.Description.Substring(0,100) </p>
        </div>
        <div class="clear"></div>
    }
       
    </div>
